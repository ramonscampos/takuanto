{
  "compilerOptions": {
    "baseUrl": "./src",
    "plugins": [
        {
          "name": "typescript-styled-plugin",
          "lint": {
            "validProperties": [
              "shadow-color",
              "shadow-opacity",
              "shadow-offset",
              "shadow-radius",
              "padding-horizontal",
              "padding-vertical",
              "margin-vertical",
              "margin-horizontal",
              "tint-color",
              "aspect-ratio",
              "elevation"
            ]
          }
        }
      ]
  },
  "include": [
      "src"
  ],
  "extends": "expo/tsconfig.base"
}
